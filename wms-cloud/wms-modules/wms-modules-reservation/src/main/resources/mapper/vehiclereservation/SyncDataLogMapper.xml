<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bosch.vehiclereservation.mapper.SyncDataLogMapper">
    <resultMap type="com.bosch.vehiclereservation.api.domain.SyncDataLog" id="SyncDataLogResult">
        <result property="id" column="id"/>
        <result property="syncStartDate" column="sync_start_date"/>
        <result property="syncEndDate" column="sync_end_date"/>
        <result property="syncLastDate" column="sync_last_date"/>
        <result property="remark" column="remark"/>
        <result property="createBy" column="create_by"/>
        <result property="updateBy" column="update_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <sql id="selectSyncDataLog">
        select id,
               sync_start_date,
               sync_end_date,
               sync_last_date,
               remark,
               create_by,
               update_by,
               create_time,
               update_time
        from vr_syncdata_log
    </sql>

    <select id="getLastSyncData" resultMap="SyncDataLogResult">
        <include refid="selectSyncDataLog"/>
        ORDER BY sync_last_date desc limit 1
    </select>
</mapper>