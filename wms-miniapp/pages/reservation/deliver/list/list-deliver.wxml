<image class="top-image" src='/img/nutricia-color.png'></image>
<view class="view-class">
  <view wx:if="{{hasData}}" > 
  <text class="title">送货预约列表</text>
  <van-cell-group wx:for="{{list}}" wx:for-item="item" wx:key="deliverId">
    <van-cell center is-link title="{{item.reserveNo}}" value="详情"
      label="{{item.reserveDate}} - {{item.timeWindow}}"
      data-cid="{{item.deliverId}}"
      bind:click="onDetails" >      
    </van-cell>
  </van-cell-group>
</view>
<view wx:if="{{!hasData}}" > 
  <van-empty description="暂无数据" />
</view>
</view>